{
    "name":         "oliverschloebe/restcord",
    "description":  "REST Library for the Discord API",
    "type":         "library",
    "require":      {
        "php":                        "^8.0",
        "guzzlehttp/guzzle":          "^7.0",
        "guzzlehttp/guzzle-services": "^1.0",
        "monolog/monolog":            "^3.6",
        "netresearch/jsonmapper":     "^1.4",
        "symfony/options-resolver":   "^3.4|^4.4|^5.1|^6.0",
        "psr/log": "^2.0|^3.0"
    },
    "autoload":     {
        "psr-4": {
            "RestCord\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RestCord\\Test\\": "tests/"
        }
    },
    "license":      "MIT",
    "authors":      [
        {
            "name":  "Aaron Scherer",
            "email": "aequasi@gmail.com"
        },
        {
            "name":  "Oliver Schl√∂be",
            "email": "info@schloebe.de"
        }
    ],
    "require-dev":  {
        "beberlei/assert":          "^2.7",
        "gossi/php-code-generator": "^0.5.0",
        "symfony/console":          "^5.4.46|^6.4.14|^7.1.7",
        "symfony/process":          "^5.4.46|^6.4.14|^7.1.7",
        "symfony/var-dumper":       "^6.4.4|^7.0.4",
        "twig/twig":                "^1.20|^2.0",
        "phpunit/phpunit":          "^8.0|^9.0"
    }
}
